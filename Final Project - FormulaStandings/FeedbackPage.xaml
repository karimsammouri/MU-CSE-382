    <ContentPage.Content>
        <StackLayout Padding="0,0,0,0">
            <WebView
                Source="https://docs.google.com/forms/d/e/1FAIpQLSeD75L0AepXHxX5l7_1MigH0tGL9lzPWBtzFYlFil-eEHfRWw/viewform?usp=sf_link"
